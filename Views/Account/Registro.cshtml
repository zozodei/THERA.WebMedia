<h1>Registrarse</h1>
<form action='@Url.Action("Registro", "Account")' method = "POST" enctype="multipart/form-data">
    
    <label>Username</label>
    <input type="text" name="username" required />
    
    <label>Contraseña</label>
    <input type="password" name="contraseña" required minlength="6"/>
    
    <label>1 si sos paciente / 2 si sos terapeuta</label>
    <input type = "number" name = "tipoDeUsuario" min = "1" max = "2"/>

    <input type ="submit" value = "Entrar">

</form>